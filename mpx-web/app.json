{
  "pages": [
    "pages/index",
    "pages/list"
  ],
  "preloadRule": {
    "pages/index": {
      "network": "all",
      "packages": [
        "packageA"
      ]
    }
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTextStyle": "white",
    "renderingMode": "mixed",
    "navigationBarTitleText": "hahahahah"
  },
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000
  },
  "useExtendedLib": {
    "weui": true
  },
  "subPackages": [
    {
      "root": "packageA",
      "pages": [
        "cat",
        "dog"
      ]
    },
    {
      "root": "packageB",
      "pages": [
        "dog",
        "cat"
      ],
      "name": "packageSecond"
    }
  ]
}